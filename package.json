{
  "name": "@geoblocks/mapboxlayer",
  "version": "0.0.4",
  "geoblocks_src": "src/MapBoxLayer.js",
  "main": "dist/lib/mapboxlayer.js",
  "module": "dist/lib/mapboxlayer.js",
  "files": [
    "/src",
    "/dist/lib"
  ],
  "scripts": {
    "build-api": "microbundle --format es --entry src/MapBoxLayer.js",
    "build-demo": "parcel build --out-dir dist examples/*.html --public-url ./",
    "build": "npm run build-api; npm run build-demo",
    "lint": "eslint src/ examples",
    "start": "parcel serve --port 8080 examples/*.html",
    "gh-pages": "rm -rf dist && npm run build && gh-pages -d dist"
  },
  "browserslist": [
    "> 1% and not IE > 0, Firefox ESR"
  ],
  "peerDependencies": {
    "mapbox-gl": "~1.0",
    "ol": "~6.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "gh-pages": "^3.0.0",
    "mapbox-gl": "^1.4.1",
    "microbundle": "^0.12.0",
    "ol": "^6.0.1",
    "parcel-bundler": "^1.12.4"
  }
}
